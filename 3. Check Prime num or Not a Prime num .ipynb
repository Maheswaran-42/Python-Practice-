    "a=int(input(\"enter the number:\"))\n",
    "def primenum(a):\n",
    "   if a <=1:\n",
    "      print(\" not a prime num\")\n",
    "   else:\n",
    "      for i in range (2,a):\n",
    "         if a%i==0:\n",
    "            print(\"not a prime num\")\n",
    "            break\n",
    "      else:\n",
    "         print(\"prime \")\n",
    "primenum(a)"
   ]
