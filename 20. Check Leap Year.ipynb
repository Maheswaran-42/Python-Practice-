   "source": [
    "a = int(input(\"Enter the year: \"))\n",
    "if a % 400 == 0:\n",
    "    print(f\"{a} is a leap year\")\n",
    "elif a % 100 == 0:\n",
    "    print(f\"{a} is not a leap year\")\n",
    "elif a % 4 == 0:\n",
    "    print(f\"{a} is a leap year\")\n",
    "else:\n",
    "    print(f\"{a} is not a leap year\")\n"
